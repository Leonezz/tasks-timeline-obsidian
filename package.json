{
	"name": "tasks-timeline-obsidian",
	"version": "0.0.1",
	"description": "This is a timeline style viewer for your tasks",
	"main": "src/main.ts",
	"type": "module",
	"scripts": {
		"dev": "cp manifest.json ExampleVault/.obsidian/plugins/tasks-timeline-obsidian/ && vite build --watch --mode=development",
		"build": "vite build && cp manifest.json ./dist/",
		"release": "git tag -a %npm_config_tag% -m '%npm_config_tag%' && git push origin %npm_config_tag%",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"lint": "eslint ."
	},
	"keywords": [],
	"devDependencies": {
		"@eslint/js": "9.30.1",
		"@types/node": "^16.11.6",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"builtin-modules": "^5.0.0",
		"esbuild": "0.25.5",
		"eslint-plugin-obsidianmd": "0.1.9",
		"globals": "14.0.0",
		"jiti": "2.6.1",
		"tslib": "2.4.0",
		"typescript": "^5.8.3",
		"typescript-eslint": "8.35.1",
		"vite": "^7.2.4"
	},
	"dependencies": {
		"@tasks-timeline/components": "github:Leonezz/tasks-timeline-components",
		"obsidian": "latest",
